{
    "manifest_version": 2,

    "name": "DuckWeChatLink",
    "description": "This extension help you open url direct when jump from wechat",
    "version": "0.1.0",

    "background": {
        "page": "background.html"
    },


    "icons": {
        "16": "icon/icon_16.png",
        "48": "icon/icon_48.png",
        "128": "icon/icon_128.png"
    },
    
    "homepage_url": "https://github.com/laobie/DuckWeChatLink",
    
    "permissions": [
        "tabs",
        "background"
    ]
}